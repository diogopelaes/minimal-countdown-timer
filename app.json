{
  "expo": {
    "name": "Minimal Countdown",
    "slug": "minimal-countdown-timer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.antigravity.minimalcountdowntimer",
      "permissions": [
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "vibrate",
        "POST_NOTIFICATIONS"
      ],
      "notifications": {
        "icon": "./assets/icon.png",
        "color": "#FF8C00",
        "importance": "high"
      },
      "extra": {
        "eas": {
          "projectId": "minimal-countdown-timer"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "kotlinVersion": "2.0.21",
            "gradleChildHeapSize": "4g"
          }
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4a889490-000d-472f-952b-02e0190983c6"
      }
    }
  }
}